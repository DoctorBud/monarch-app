    <div class="wrapperforfooter">
      <div class="container-fluid monarch-container">
        <div class="content">

            <div id="complete-info">
                <h1 class="downloadline">Search Results: {{term}}</h1>
                <a href="{{{download.json}}}" class="downloadline btn btn-info">JSON</a>

                {{#hasResults}}

                <div class="filters">

                  <div class="search-results-ui-group">
                    <strong>Species</strong>
                    {{{resultsSpeciesFilter}}}
                  </div>

                  <div class="search-results-ui-group">
                    <strong>Category</strong>
                    {{{resultsCategoryFilter}}}
                  </div>

                </div>

                <div class="search-results-grid">
                  {{{resultsOutput}}}
                </div>

                {{/hasResults}}
                {{^hasResults}}
                    {{{suggestSpan}}}
                {{/hasResults}}
            </div>
        </div>
      </div>
    </div> <!-- closes wrapperforfooter -->
