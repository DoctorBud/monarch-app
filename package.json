{
  "name": "webpack-babel-starter",
  "version": "2.0.1",
  "description": "Webpack Babel Starter Kit",
  "scripts": {
    "clean": "npm run oldclean",
    "dev": "npm run olddev",
    "build": "npm run oldbuild",
    "start": "npm run oldstart",
    "oldclean": "rm -rf ./dist/*",
    "olddev": "USE_LOG_FETCH=0 USE_BUNDLE=1 USE_WEBPACK=1 NODE_PATH=./lib/monarch node_modules/.bin/nodemon -d 2 -V -- lib/monarch/web/webapp_launcher.js & node_modules/.bin/webpack-dev-server --config ./webpack.config.js.old --port 8081 --history-api-fallback --hot --inline",
    "oldbuild": "npm run oldclean && ./node_modules/webpack/bin/webpack.js --config webpack.build.js --bail -p",
    "oldstart": "USE_BUNDLE=1 NODE_PATH=./lib/monarch node lib/monarch/web/webapp_launcher.js",
    "wbs-analyze": "npm run wbs-clean-dist && ANALYZE=1 node_modules/.bin/webpack --bail --progress --colors",
    "wbs-postinstall": "node ./wpcontributing/bin/install-warning-message.js",
    "wbs-start": "npm run webpack",
    "wbs-test": "npm run wbs-lint",
    "wbs-lint": "./node_modules/.bin/eslint wpsrc",
    "wbs-lint-fix": "./node_modules/.bin/eslint --fix wpsrc --ext .js",
    "wbs-clean-dist": "node ./wpbin/clean-dist.js",
    "wbs-serve-build": "echo 'Serving distribution folder build/dist' && npm run wbs-serve-dist",
    "wbs-serve-dist": "./node_modules/.bin/serve build/dist",
    "wbs-build": "npm run wbs-clean-dist && npm run wbs-webpack-build",
    "wbs-build-prod": "npm run wbs-clean-dist && NODE_ENV=production npm run wbs-webpack-build-prod",
    "wbs-build-prod-all": "DEVTOOLS=false npm run wbs-build-prod && NODE_ENV=production OPTIMIZE=false DEVTOOLS=true DIST_DIR=dist/devtools npm run wbs-webpack-build",
    "wbs-postbuild-prod-all": "npm run wbs-generate-dist-readme",
    "wbs-webpack": "./node_modules/.bin/webpack-dev-server --progress --colors --hot --inline",
    "wbs-webpack-build": "./node_modules/.bin/webpack --progress",
    "wbs-webpack-build-prod": "./node_modules/.bin/webpack --progress -p",
    "wbs-generate-dist-readme": "node wpbin/generate-dist-readme.js",
    "wbs-contributing-install": "./wpcontributing/bin/contributing-install.sh",
    "wbs-contributing-test": "./wpcontributing/bin/contributing-test.sh"
  },
  "pre-commit": [
    "test"
  ],
  "keywords": [
    "Webpack",
    "Babel",
    "ES6",
    "Sass",
    "Eslint",
    "Starter Kit",
    "boilerplate"
  ],
  "author": "Christophe Rosset <tophe@topheman.com> (http://labs.topheman.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/topheman/webpack-babel-starter.git"
  },
  "bugs": {
    "url": "https://github.com/topheman/webpack-babel-starter/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.5.0",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chai": "^4.0.2",
    "css-loader": "^0.28.4",
    "del": "^2.2.0",
    "eslint": "4",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "7",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "2",
    "file-loader": "^0.11.2",
    "git-rev-sync": "^1.4.0",
    "gulp": "^3.9.1",
    "gulp-download": "0.0.1",
    "gulp-jsoncombine": "^1.0.3",
    "gulp-mocha": "^4.3.1",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.6.3",
    "gulp-uglify": "^3.0.0",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.7",
    "less-loader": "^4.0.5",
    "lodash.template": "^4.2.2",
    "mocha": "^3.4.2",
    "moment": "^2.11.2",
    "my-local-ip": "^1.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npmlog": "^2.0.2",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.1.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "serve": "^1.4.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.4.5"
  },
  "private": true,
  "dependencies": {
    "async": "^2.5.0",
    "bbop": "^2.4.3",
    "bbop-graph": "0.0.19",
    "bbop-manager-golr": "0.0.12",
    "bbop-registry": "0.0.3",
    "bbop-response-golr": "0.0.8",
    "bbop-rest-manager": "0.0.11",
    "bbop-widget-set": "^2.5.4",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "golr-conf": "0.0.3",
    "hapi": "^16.5.2",
    "inert": "^4.2.1",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "mustache": "^2.3.0",
    "phenogrid": "^1.3.11",
    "pup-tent": "^0.9.10",
    "request": "^2.81.0",
    "underscore": "^1.8.3",
    "wait.for": "^0.6.6",
    "xml2js": "^0.4.17"
  }
}
