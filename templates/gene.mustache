<html>
  <head>
    <title>Monarch Gene: {{label}}</title>
  </head>
  <body>
    <h1>{{label}}</h1>
    <div id="description">
      <p>
        {{description}}
      </p>
    </div>
    <div class="details">
      <b class="detail-block-header">Diseases</b>
      <div class="detail-block-contents">
        <table>
          {{{phenotypeTable}}}
        </table>
      </div>
    </div>
    <div class="details">
      <b class="detail-block-header">Phenotypes</b>
      <div class="detail-block-contents">
        <table>
          {{{phenotypeTable}}}
        </table>
      </div>
    </div>
    <div class="details">
      <b class="detail-block-header">Interacting Genes</b>
      <div class="detail-block-contents">
        <table>
          {{{geneTable}}}
        </table>
      </div>
    </div>
    <div class="details">
      <b class="detail-block-header">Alleles</b>
      <div class="detail-block-contents">
        <table>
          {{{alleleTable}}}
        </table>
      </div>
    </div>
  </body>
</html>
