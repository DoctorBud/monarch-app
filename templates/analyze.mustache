<!DOCTYPE html>
<html>
  <head>
    <title>Monarch Analysis: {{datatype}}</title>
    {{#scripts}}
    <script type="text/javascript" src="{{url}}"></script>
    {{/scripts}}
    {{#stylesheets}}
    <link rel="stylesheet" type="text/css" href="{{url}}"></link>
    {{/stylesheets}}
    <meta charset=”utf-8”> 
  </head>
  <body>
    <div class="container">
      {{{includes.navbar}}}
      <div class="row">
        <div class="col-lg-2">
          {{{includes.navlist}}}
        </div>
        <div class="col-lg-9">

          <h1>Monarch Analysis: {{datatype}}</h1>
          <span class="text-warning">DOES NOT WORK YET DON'T GET YOUR
          HOPES UP!!</span>

          <form  action="/analyze/{{datatype}}/" method="GET">  
            <textarea rows="10" column="20" name="input_items">
              {{input_items}}
            </textarea>
            <button type="submit" class="btn btn-primary">Analyze!</button>  
            <br/>
            Max number of results: <input name="limit" type="text">{{limit}}</input>
          </form> 
	  <a href="{{{base_url}}}.json" class="btn btn-success">JSON</a>
          <h3>Results</h3>
          <table class="{{css.table}}">
            {{{resultsTable}}}
          </table>
        </div>
      </div>
    </div>
  </body>
</html>
