<!DOCTYPE html>
<html>
  <head>
    <title>blog-scratch</title>
    {{#scripts}}
    <script type="text/javascript" src="{{url}}"></script>
    {{/scripts}}
    {{{includes.analytics}}}
    {{#stylesheets}}
    <link rel="stylesheet" type="text/css" href="{{url}}"/>
    {{/stylesheets}}
    <link rel="stylesheet" type="text/css" href="/css/tour.css"/>
    <link rel="icon" href="/image/favicon.ico" type="image/x-icon">
    <meta charset="utf-8"/> 
  </head>
  <body>
    <div class="monarch-container">
      {{{includes.navbar}}}
      <div class="content">
        <div class="wrapperforfooter">
          <div class="title">blog scratch</div>

	  <div style="margin-top: 1em;">
	    <h4>Server-side implementation</h4>
	    {{#blog_results}}
	    <dl>
	      <dt>
		<a href="{{link}}">{{title}}</a>
		{{#category}}
		<a href="{{link}}" class="label label-default">{{label}}</a>
		{{/category}}
	      </dt>
	      <dd>
		<strong>{{date}}</strong>
		{{description}}
	      </dd>
	    </dl>
	    {{/blog_results}}
	  </div>

	</div> <!-- closes wrapperforfooter -->
	{{{includes.footer}}}
      </div>
    </div>
  </body>
</html>
