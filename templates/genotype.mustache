<!DOCTYPE html>
<html>
<head>
    <title>Monarch Genotype: {{label}} ({{id}})</title>
    {{#scripts}}
    <script type="text/javascript" src="{{url}}"></script>
    {{/scripts}}
    <script src="/js/tabs.js"></script>
    <script src="/js/stupidtable.min.js"></script>
    <script src="/js/tables.js"></script>
    {{#stylesheets}}
    <link rel="stylesheet" type="text/css" href="{{url}}"/>
    {{/stylesheets}}
    <link rel="stylesheet" type="text/css" href="/css/monarch-specific.css"/>
    <link rel="stylesheet" type="text/css" href="/css/imagehover.css"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta charset="utf-8"/>
</head>
<body>
    {{{includes.navbar}}}
    <div class="monarch-container">
        <div class="sidebar">
            <a href="/"><img src="/image/logo-sneak.png" width="100" class="img-rounded"/></a>
            <ul class="nav nav-list">
                <li class="nav-header">Find</li>
                    <li class="nav-item"><a href="/disease">Disease</a></li>
                    <li class="nav-item"><a href="/phenotype">Phenotype</a></li>
                    <li class="nav-item"><a href="/model">Model</a></li>
                <li id="navigation" class="nav-header">Navigate</li>
                    <li id="over" class="special nav-item"><a href="#overview">Overview</a></li>
                    {{#hasPhenotypes}}
                    <li id="phen" class="special nav-item"><a href="#phenotypes">Phenotypes</a></li>
                    {{/hasPhenotypes}}
                    {{#hasDiseases}}
                    <li id="dis" class="special nav-item"><a href="#disease">Disease</a></li>
                    {{/hasDiseases}}
                    {{#hasGenes}}
                    <li id="gen" class="special nav-item"><a href="#genes">Genes</a></li>
                    {{/hasGenes}}
                    {{#hasSim}}
                    <li id="simi" class="special nav-item"><a href="#similarity">Similarity</a></li>
                    {{/hasSim}}
            </ul>
        </div>
        <div class="content">
            <div class="jumbotron">
                <div id="description">
                    <h1 class="downloadline">Genotype: {{label}}</h1>
                    <a href="{{{base_url}}}.json" class="btn btn-info downloadline">JSON</a>
                    <!-- <a href="{{{base_url}}}.rdf" class="btn btn-success">RDF</a> -->
    <!--                 <a href="{{{base_url}}}.nt" class="btn btn-info">n-triples</a> -->
                    <p>
                        ID: {{taxon.id}}<br/>
                        Label: {{taxon.label}}
                    </p>
                </div>
                <div id="categories">
                    <a href="#overview"><div id="initialtab" class="tab contenttab overtab">Overview</div></a>
                    {{#hasPhenotypes}}                
                    <a href="#phenotypes"><div class="tab contenttab phentab">Phenotypes ({{phenotypeNum}})</div></a>
                    {{/hasPhenotypes}}
                    {{^hasPhenotypes}}
                    <div class="tab greyed-out">Phenotypes (0)</div>
                    {{/hasPhenotypes}}
                    {{#hasDiseases}}
                    <a href="#disease"><div id="longertab" class="tab contenttab distab">Related Diseases ({{diseaseNum}})</div></a>
                    {{/hasDiseases}}
                    {{^hasDiseases}}
                    <div class="tab greyed-out">Related Diseases (0)</div>
                    {{/hasDiseases}}
                    <div class="second-row-divider"></div>
                    {{#hasGenes}}
                    <a href="#genes"><div class="tab contenttab gentab second">Genes ({{geneNum}})</div></a>
                    {{/hasGenes}}
                    {{^hasGenes}}
                    <div class="tab greyed-out second">Genes (0)</div>
                    {{/hasGenes}}
                    {{#hasSim}}
                    <a href="#similarity"><div class="tab contenttab simitab">Similar Models ({{simNum}})</div></a>
                    {{/hasSim}}
                    {{^hasSim}}
                    <div class="tab greyed-out">Similar Models (0)</div>
                    {{/hasSim}}
                </div>
            </div>
            <div id="complete-info">
                <div id="overview" class="first category">
                    <h3>Overview</h3>
                        <div class="ids">
                            <b>ID:</b> {{id}}
                        </div>
                </div>
                {{#hasPhenotypes}}
                <div id="phenotypes" class="category">
                    <h3 class="downloadline">Phenotypes</h3>
                    <a href="{{{base_url}}}/phenotype_associations.json" class="btn btn-info downloadline">JSON</a>
    <!--                 <a href="{{{base_url}}}/phenotype_associations.rdf" class="btn btn-success">RDF</a> -->
    <!--                 <a href="{{{base_url}}}/phenotype_associations.nt" class="btn btn-success">n-triples</a> -->
                    <table class="{{css.table}}">
                    {{{phenotypeTable}}}
                    </table>
                </div>
                {{/hasPhenotypes}}
                {{#hasDiseases}}
                <div id="disease" class="category">
                    <h3>Related Diseases</h3>
                    <table class="{{css.table}}">
                    {{{diseaseTable}}}
                    </table>
                </div>
                {{/hasDiseases}}
                {{#hasGenes}}
                <div id="genes" class="category">
                    <h3>Genes</h3>
                    <table class="{{css.table}}">
                    {{{geneTable}}}
                    </table>
                </div>
                {{/hasGenes}}
                {{#hasSim}}
                <div id="similarity" class="category">
                    <h3>Similar Models</h3>
                    <table class="{{css.table}}">
                    {{{simTable}}}
                    </table>
                </div>
                {{/hasSim}}
            </div>
            {{{includes.footer}}}
        </div>
    </div>
</body>
</html>
