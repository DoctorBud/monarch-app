                    <strong>Category</strong>
                    {{{resultsCategoryFilter}}}
                  </div>

                </div>

</div>

<div class="col-xs-12 col-md-9">

<div class="search-results-rows">
                  {{{resultsOutput}}}
                </div>

</div>
</div>



                {{/hasResults}}
                {{^hasResults}}
                    {{{suggestSpan}}}
                {{/hasResults}}
            </div>
        </div>
      </div>
