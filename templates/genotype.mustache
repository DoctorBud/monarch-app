<html>
<head>
    <title>Monarch Genotype: {{label}} ({{id}})</title>
    {{#scripts}}
    <script type="text/javascript" src="{{url}}"></script>
    {{/scripts}}
    {{#stylesheets}}
    <link rel="stylesheet" type="text/css" href="{{url}}"></link>
    {{/stylesheets}}
    <link rel="stylesheet" type="text/css" href="../css/monarch-specific.css"></link>
    <link rel="stylesheet" type="text/css" href="../css/imagehover.css"></link>
    <script src="../js/tabs.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    {{{includes.navbar}}}
    <div class="monarch-container">
    <div class="sidebar">
        {{{includes.navlistgenotype}}}
    </div>
    <div class="content">
        <div class="jumbotron">
            <div id="description">
                <h1 class="downloadline">Genotype: {{label}}</h1>
                <a href="{{{base_url}}}.json" class="btn btn-info downloadline">JSON</a>
                <!-- <a href="{{{base_url}}}.rdf" class="btn btn-success">RDF</a> -->
<!--                 <a href="{{{base_url}}}.nt" class="btn btn-info">n-triples</a> -->
            </div>
            <div id="categories">
                <a href="#overview"><div id="initialtab" class="tab overtab">Overview</div></a>
                <a href="#phenotypes"><div class="tab phentab">Phenotypes</div></a>
                <a href="#disease"><div class="tab distab">Disease</div></a>
                <a href="#genes"><div class="tab gentab">Genes</div></a>
                <a href="#similarity"><div class="tab simtab">Similarity</div></a>
            </div>
        </div>
        <div id="complete-info">
            <div id="overview" class="first category">
                <h3>Overview</h3>
                    <div class="ids">
                        <b>URI:</b> <a href="{{{url}}}">{{{url}}}</a>
                    </div>
                    <div class="ids">
                        <b>ID:</b> {{id}}
                    </div>
            </div>
            <div id="phenotypes" class="category">
                <h3 class="downloadline">Phenotypes</h3>
                <a href="{{{base_url}}}/phenotype_associations.json" class="btn btn-info downloadline">JSON</a>
<!--                 <a href="{{{base_url}}}/phenotype_associations.rdf" class="btn btn-success">RDF</a> -->
<!--                 <a href="{{{base_url}}}/phenotype_associations.nt" class="btn btn-success">n-triples</a> -->
                <table class="{{css.table}}">
                {{{phenotypeTable}}}
                </table>
            </div>
            <div id="disease" class="category">
                <h3>Associated Diseases</h3>
                <table class="{{css.table}}">
                {{{diseaseTable}}}
                </table>
            </div>
            <div id="genes" class="category">
                <h3>Genes</h3>
                <table class="{{css.table}}">
                {{{geneTable}}}
                </table>
            </div>
            <div id="similarity" class="category">
                <h3>Similar Diseases</h3>
                <table class="{{css.table}}">
                {{{simTable}}}
                </table>
            </div>
        </div>
        {{{includes.footer}}}
    </div>
</body>
</html>
